{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "react-app",
      "plugin:react-hooks/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "react",
      "react-hooks"
    ],
    "rules": {
      "react/prop-types": "warn"
    }
  }
  